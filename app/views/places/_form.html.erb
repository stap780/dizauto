<%= turbo_frame_tag dom_id(@warehouse, dom_id(place))  do %>
    <%= form_with(model: [@warehouse, place], class: "row justify-content-between align-items-center mb-3", data: {}) do |f|%>
        <%= render 'shared/errors', object: place %>
        <div class="col-5">
          <%# f.label :sector, class: "col-form-label" if f.object.new_record?%>
          <%= f.text_field :sector, class: "form-control" %>
        </div>
        
        <div class="col-5">
          <%# f.label :cell, class: "col-form-label" if f.object.new_record?%>
          <%= f.text_field :cell, class: "form-control" %>
        </div>

        <div class="col d-flex justify-content-end gap-2 align-items-center">
            <%= f.submit t(:save), class: "btn btn-primary" %>
            <%= link_to close_icon, :back, class: "btn btn-outline-primary" %>
        </div>

    <% end %>
<% end %>