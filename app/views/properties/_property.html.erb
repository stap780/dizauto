<tr id="<%= dom_id property %>">
    <td><b><%= property.id%></b></td>
    <td><%= property.title %></td>
    <td>
        <%= link_to '<i class="bi bi-eye"></i>'.html_safe, property_path(property), class: 'px-2' %>
        <%= link_to '<i data-feather="plus-circle"></i>'.html_safe, new_property_characteristic_path(property) %>
        <%= property.characteristics.pluck(:title).take(10).join(',')+' ...' if  property.characteristics.present? %>
    </td>
    <%= td_index_edit_delete_link(property)%>
</tr>