<%= turbo_frame_tag insale, class: "py-2 px-0 justify-content-between align-items-center py-2 border-top-1 border-start-0 border-end-0 row", data: { }  do %>

  <div class="card w-50">
    <div class="card-header"></div>
    <div class="card-body">
      <div class="mb-3 row align-items-center">
        <label class="col-form-label col-sm-2 text-sm-end" for="insale_api_key">ключ</label>
        <div class="col-sm-10 col-md-8"><%= insale.api_key %></div>
      </div>
      <div class="mb-3 row">
        <label class="col-form-label col-sm-2 text-sm-end" for="insale_api_password">пароль</label>
        <div class="col-sm-10 col-md-8"><%= insale.api_password %></div>
      </div>
      <div class="mb-3 row align-items-center">
        <label class="col-form-label col-sm-2 text-sm-end" for="insale_api_link">ссылка</label>
        <div class="col-sm-10 col-md-8"><%= insale.api_link %></div>
      </div>
    </div>
  </div>

<% end %>