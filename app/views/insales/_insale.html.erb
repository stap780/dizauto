
<%=turbo_frame_tag insale, class: "d-table-row" do %>
    <%= tag.div( insale.id, class: "d-table-cell") %>
    <%= tag.div( insale.api_key, class: "d-table-cell")%>
    <%= tag.div( insale.api_password, class: "d-table-cell")%>
    <%= tag.div( insale.api_link, class: "d-table-cell")%>
    <%= tag.div class: "d-table-cell" do %>
        <%= tag.div class: h_v_center do %>
            <%=link_to edit_icon, edit_insale_path(insale), class: "btn", data: { turbo_frame: 'offcanvas_wrap', turbo_prefetch: false } %>
            <%=link_to "Проверить интеграцию", check_insales_path, class: "btn btn-primary me-1", data: {turbo_stream: true}%>
            <%=link_to "Вебхук", add_order_webhook_insales_path, class: "btn btn-primary", data: {turbo_stream: true}%>
            <%=link_to trash_icon, insale, data: { "turbo-method": :delete, 'turbo-confirm': 'Are you sure?' }, class: "btn" %> 
        <%end %>
    <%end %>
<% end %>