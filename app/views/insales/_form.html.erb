<%= turbo_frame_tag insale do %>
  <%= form_with(model: insale) do |f| %>
    <%= render "shared/errors", object: insale %>
    <div class="card w-50">
      <div class="card-header"></div>
      <div class="card-body">
          <div class="mb-3 row">
            <%= f.label :api_key, class: "col-form-label col-sm-2 text-sm-end" %>
            <div class="col-sm-10 col-md-8">
              <%= f.text_field :api_key, class: "form-control", placeholder: "api_key" %>
            </div>
          </div>
          <div class="mb-3 row">
            <%= f.label :api_password, class: "col-form-label col-sm-2 text-sm-end" %>
            <div class="col-sm-10 col-md-8">
              <%= f.text_field :api_password, class: "form-control", placeholder: "api_password" %>
            </div>
          </div>
          <div class="mb-3 row">
            <%= f.label :api_link, class: "col-form-label col-sm-2 text-sm-end" %>
            <div class="col-sm-10 col-md-8">
              <%= f.text_field :api_link, class: "form-control", placeholder: "api_link" %>
            </div>
          </div>
          <div class="mb-3 row">
            <div class="col-sm-10 ms-sm-auto d-flex gap-2 align-items-center">
              <%= f.submit t('save'), class: "btn btn-primary" %>
              <%= link_to close_icon, :back, class: "btn btn-outline-primary" %>
            </div>
          </div>
      </div>
    </div>

  <% end %>
<% end %>