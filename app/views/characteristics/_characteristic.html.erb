<tr id="<%= dom_id characteristic %>">
  <td><b>#<%= characteristic.id%></b></td>
  <td><%= characteristic.title %></td>
  <td>
    <%= link_to '<i data-feather="edit-2"></i>'.html_safe, edit_property_characteristic_path(@property, characteristic), class: 'px-2' %>
    <%= link_to '<i data-feather="trash-2"></i>'.html_safe, [@property, characteristic], data: { "turbo-method": :delete , confirm: 'Are you sure?' } %>
  </td>
</tr>