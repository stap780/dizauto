<%= form_with(model: @user, url: users_admin_create_path, data: {controller: 'user', 'user-dnone-class': 'd-none'}) do |f| %>
    <%= render "shared/errors", object: @user %>


    <%= tag.div class: 'card' do %>
        <%= tag.div class: 'card-header' %>
        <%= tag.div class: 'card-body row' do %>
            <%= render 'form_data', f: f %>
            <%= render 'nested_permission', f: f %>
        <% end %>
        <%= tag.div class: 'card-footer' do %>
            <%= f.submit class: "btn btn-primary" %>
            <%= link_to "Back", users_path, class: "btn btn-outline-primary" %>
        <% end %>
    <% end %>

<% end %>
