<%= form_with(model: @permission) do |f| %>
    <%= render "shared/errors", object: @permission %>

  <div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">
            </div>
            <div class="card-body">
              <div class="row">

                <%= render 'form_data', f:f %>

              </div>            
            </div>
            <div class="card-footer">
                <%= f.submit class: "btn btn-primary" %>
                <%= link_to t('back'), :back, class: "btn btn-outline-primary" %>
            </div>
        </div>
    </div>
</div>

<% end %>
